{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": ["error", 3],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "max-len": ["error", { "code": 150 }]
    }
}
